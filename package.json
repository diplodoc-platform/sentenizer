{
  "name": "@diplodoc/sentenizer",
  "version": "0.0.9",
  "description": "text segmentation into sentences",
  "homepage": "https://github.com/diplodoc-platform/sentenizer",
  "repository": {
    "type": "git",
    "url": "git@github.com:diplodoc-platform/sentenizer.git"
  },
  "engines": {
    "npm": ">=11.5.1"
  },
  "bugs": {
    "url": "https://github.com/diplodoc-platform/sentenizer/issues",
    "email": "moki-codes@yandex-team.ru"
  },
  "keywords": [
    "nlp",
    "parser",
    "tokenization",
    "sentence-segmentation",
    "russian"
  ],
  "main": "./lib/index.js",
  "types": "./lib/index.d.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "build": "npm run build:clean && npm run build:code",
    "build:code": "tsc -p tsconfig.types.json && node esbuild/build.js",
    "build:clean": "rm -rf lib",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "lint": "lint update && lint",
    "lint:fix": "lint update && lint fix",
    "pre-commit": "lint update && lint-staged",
    "prepare": "husky"
  },
  "author": {
    "name": "moki-codes",
    "email": "moki-codes@yandex-team.ru",
    "url": "github.com/moki"
  },
  "license": "MIT",
  "devDependencies": {
    "@diplodoc/lint": "^1.13.2",
    "@diplodoc/tsconfig": "^1.0.2",
    "@esbuild-plugins/tsconfig-paths": "^0.1.2",
    "@types/node": "^22.19.7",
    "@types/ramda": "^0.28.13",
    "@vitest/coverage-v8": "^3.0.8",
    "typescript": "^5.0.4",
    "vite": "^6.0.5",
    "vitest": "^3.0.8"
  },
  "dependencies": {
    "ramda": "^0.28.0"
  }
}
